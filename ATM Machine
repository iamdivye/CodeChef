import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner in = new Scanner(System.in);
		int t = in.nextInt();
		for(int j=0;j<t;j++)
		{
		    int n = in.nextInt();
		int k = in.nextInt();
		String s="";
		int x;
		for(int i=0;i<n;i++)
		{
		    x=in.nextInt();
		    if(x<=k)
		    {
		        s=s+"1";
		        k-=x;
		    }
		    else
		    s=s+"0";
		}
		System.out.println(s);
		}
		
	}
}
